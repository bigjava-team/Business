<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.bigjava.notice.entity.IndexNotice" table="indexnotice">
		<id name="indexId">
			<generator class="native" />
		</id>
		<property name="indexNaticeTitle" />
		<property name="indexNatice" />
		<property name="indexNaticeTime" />
	</class>
</hibernate-mapping>
